<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Ejecutar debitos -->

    <record id="mobbex_execution_wizard" model="ir.ui.view">
        <field name="name">Ejecutar cobros</field>
        <field name="model">mobbex.execution.wizard</field>
        <field name="arch" type="xml">
					<form>
						<footer>
							<button name="confirmar_ejecucion" string="Confirmar" class="oe_right oe_highlight" type="object"/>
							<button string="Cancel" class="oe_right" special="cancel"/>
						</footer>
					</form>
        </field>
    </record>

		<act_window id="action_mobbex_execution"
			name="Ejecutar suscripciones"
			src_model="financiera.mobbex.execution"
			res_model="mobbex.execution.wizard"
			view_id="mobbex_execution_wizard"
			view_type="form" view_mode="form"
			key2="client_action_multi" target="new"
			groups="financiera_mobbex.ejecutar_suscripcion_manual"
		/>

</odoo>